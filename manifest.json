{
  "manifest_version": 3,
  "name": "Move On Up",
  "version": "1.5.0",
  "description": "Quickly go up one level in any URL. Click the icon or use a shortcut. Smart modes, context menu, and instant customization. ðŸš€",
  "icons": {
    "16": "assets/icon.png",
    "32": "assets/icon.png",
    "48": "assets/icon.png",
    "128": "assets/icon.png"
  },
  "permissions": [
    "tabs",
    "storage",
    "contextMenus",
    "notifications"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": { "default_icon": "assets/icon.png" },
  "options_ui": {
    "page": "options/options.html",
    "open_in_tab": false
  },
  "commands": {
    "move-up": {
      "suggested_key": {
        "default": "Ctrl+Shift+U",
        "mac": "Command+Shift+U"
      },
      "description": "Move up in the URL path"
    }
  }
}